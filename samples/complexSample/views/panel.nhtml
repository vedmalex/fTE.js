<#@ alias "panel" #>
<#@ context 'panel' #>
<#- block 'title' : -#>
<#@ context 'title' #>
  <div class="panel-heading">
    <h3 class="panel-title">#{title}</h3>
  </div>
<#- end -#>
<#- block 'body' : -#>
<#@ context 'body' #>
  <div class="panel-body">
    #{body}
  </div>
<#- end -#>
<div class="panel panel-default">
#{content('title', panel.title)}
#{content('body', panel.body)}
#{content()}
</div>